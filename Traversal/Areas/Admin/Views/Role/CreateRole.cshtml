@model CreateRoleViewModel
@{
    ViewData["Title"] = "CreateRole";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
   
}
<h1>Yeni Rol Oluşturma Sayfası</h1>
<form method="post">
    <div class="form-group">
        <label>Rol Adını Giriniz</label>
        <input type="text" asp-for="RoleName" class="form-control" />
    </div>
    <button class="btn btn-outline-success">Rol Ekle</button>
</form>